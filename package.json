{
  "name": "kb-exporter",
  "version": "1.0.0",
  "description": "Experimental CLI that bulk-exports Salesforce Knowledge articles, rewrites embedded HTML so images point to local files, and bundles everything into a ready-to-import ZIP package.",
  "main": "src/main.js",
  "scripts": {
    "lint": "eslint --ext .js src test",
    "lint:fix": "eslint --ext .js src test --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "format": "prettier \"src/**/*.{js,json,md}\" --write",
    "format:check": "prettier \"src/**/*.{js,json,md}\" --check"
  },
  "keywords": [
    "salesforce",
    "knowledge",
    "knowledge-base",
    "export",
    "cli",
    "jsforce",
    "node",
    "solid",
    "html",
    "images",
    "zip"
  ],
  "dependencies": {
    "ansi-colors": "^4.1.3",
    "archiver": "^7.0.1",
    "axios": "^1.4.0",
    "cheerio": "^1.0.0-rc.12",
    "cli-progress": "^3.12.0",
    "csv-parser": "^3.0.0",
    "file-type": "^20.4.1",
    "jsforce": "^3.7.0",
    "ora": "^5.4.1",
    "yargs": "^17.7.2"
  },
  "author": "Pedro Ayensa",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3"
  }
}
